$onlyIf[$isBot[$mentioned[1;yes]]!=true;No puedo ver la informaci√≥n de un bot]
$authorIcon[$userAvatar[$mentioned[1;yes]]]
$thumbnail[$userAvatar[$mentioned[1;yes]]]
$author[$username[$mentioned[1;yes]]]

$addField[Name;$username[$mentioned[1;yes]];yes]

$addField[Discriminator;#$discriminator[$mentioned[1;yes]];yes]

$addField[Full Name;$username[$mentioned[1;yes]]#$discriminator[$mentioned[1;yes]];yes]

$addField[Created In;$userJoinedDiscord[$mentioned[1;yes]];yes]

$addField[Join Server in;$userJoined[$mentioned[1;yes]];yes]

$addField[DM Status;$replaceText[$replaceText[$isUserDMEnabled[$mentioned[1;yes]];false;Closed;1];true;Open;1];yes]

$addField[Status User;$getUserStatus[$mentioned[1;yes]]]

$addField[Permisos;
$replaceText[$replaceText[$checkCondition[$isAdmin[$mentioned[1;yes]]==true];true;Administrador del server;-1];false;Normal User;-1]]
$footer[Information From $username[$mentioned[1;yes]]#$discriminator[$mentioned[1;yes]]]
